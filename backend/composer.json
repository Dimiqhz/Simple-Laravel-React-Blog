{
  "name": "blog/blog-app",
  "description": "Простой блог на Laravel 12",
  "type": "project",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeders",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
