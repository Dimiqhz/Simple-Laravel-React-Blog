{
  "name": "blog/backend",
  "description": "Простой блог на Laravel 12",
  "type": "project",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21",
    "laravel/pint": "^1.0",
    "laravel/sail": "^1.0",
    "mockery/mockery": "^1.5",
    "nunomaduro/collision": "^7.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/Helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
